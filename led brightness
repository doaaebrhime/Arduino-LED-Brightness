int led=3;
void setup() {
  // put your setup code here, to run once:
pinMode(led,OUTPUT);
}

void loop() {
  // put your main code here, to run repeatedly:
analogWrite(led,0);
  delay(500);
  analogWrite(led,65);
  delay(500);
analogWrite(led,128);
  delay(500);
  analogWrite(led,192);
  delay(500);
  analogWrite(led,255);
  delay(500);
}
